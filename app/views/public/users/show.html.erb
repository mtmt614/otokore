<h2>my page</h2>
    <div>
      <%= image_tag @user.get_profile_image(100,100) %>
      <h5><%= @user.name %></h5>
      <% if @user == current_user %>
      <p><%= link_to "プロフィール編集", edit_user_path %></p>
      <% end %>
    </div>

          <table class="table">
            <th>曲名</th>
            <th>アーティスト</th>
            <th>ジャンル</th>
          </tr>
        <tbody>
          <% @posts.each do |post| %>
          <tr>
            <td><%= link_to post.title, post_path(post)  %></td>
            <td><%= post.artist %></td>
            <td><%= post.genre.genrename %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
      <%= paginate @posts %>